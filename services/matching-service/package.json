{
  "name": "matching-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint src/**/*.{ts,js} swagger-doc-gen.ts",
    "build": "yarn run swagger-autogen && tsc",
    "start": "node ./dist/src/app.js",
    "dev:local": "dotenv -e ../../.env -c development -- yarn dev",
    "dev": "yarn run swagger-autogen && ts-node-dev src/app.ts",
    "swagger-autogen": "ts-node swagger-doc-gen.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "morgan": "~1.9.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/morgan": "^1.9.5",
    "ts-node-dev": "^2.0.0"
  }
}
