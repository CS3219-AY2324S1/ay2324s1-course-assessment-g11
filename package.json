{
  "private": true,
  "workspaces": {
    "packages": [
      "services/*",
      "frontend"
    ],
    "nohoist": [
      "**/@prisma/client"
    ]
  },
  "scripts": {
    "docker:build": "docker build -t peerprep-base -f Dockerfile . && dotenv -e .env -- docker-compose build",
    "docker:devup": "dotenv -e .env -- docker-compose up",
    "docker:devdown": "docker-compose down"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.19",
    "@babel/preset-env": "^7.22.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.7",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.49.0",
    "firebase-tools": "^12.6.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.4.2",
    "shx": "^0.3.4",
    "swagger-autogen": "^2.23.6",
    "swagger-express-ts": "^1.1.0"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "swagger-ui-express": "^5.0.0"
  }
